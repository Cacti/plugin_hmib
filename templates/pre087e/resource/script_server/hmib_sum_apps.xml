<interface>
	<name>Host MIB Application Graphs</name>
	<script_path>|path_cacti|/scripts/ss_hmib_sum_apps.php</script_path>
	<script_function>ss_hmib_sum_apps</script_function>
	<script_server>php</script_server>
	<arg_prepend></arg_prepend>
	<arg_index>index</arg_index>
	<arg_query>query</arg_query>
	<arg_get>get</arg_get>
	<output_delimeter>!</output_delimeter>
	<index_order>appName</index_order>
	<index_order_type>alphanumeric</index_order_type>
	<index_title_format>|chosen_order_field|</index_title_format>

	<fields>
		<appName>
			<name>Application Name</name>
			<direction>input</direction>
			<query_name>appName</query_name>
		</appName>
		<appRunning>
			<name>Number Running</name>
			<direction>output</direction>
			<query_name>running</query_name>
		</appRunning>
		<appMemory>
			<name>Running Memory</name>
			<direction>output</direction>
			<query_name>perfMemory</query_name>
		</appMemory>
		<appAvgMemory>
			<name>Average Running Memory</name>
			<direction>output</direction>
			<query_name>perfAvgMemory</query_name>
		</appAvgMemory>
		<appMaxMemory>
			<name>Maximum Running Memory</name>
			<direction>output</direction>
			<query_name>perfMaxMemory</query_name>
		</appMaxMemory>
		<appCpu>
			<name>Total CPU</name>
			<direction>output</direction>
			<query_name>perfCpu</query_name>
		</appCpu>
	</fields>
</interface>
